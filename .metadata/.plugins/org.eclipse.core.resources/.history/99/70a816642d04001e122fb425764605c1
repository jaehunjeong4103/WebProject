package entity;

import java.util.Date;

public class Reservation {
	private int id;
	private String member_id;
	private int group_id;
	private String stay_name;
	private String experience_name;
	private int code;
	private String reservation_date;
	private int reservation_person;
	private int price;
	
	public Reservation() {
	}

	public Reservation(int id, String member_id, int group_id, String stay_name, String experience_name, int code,
			String reservation_date, int reservation_person, int price) {
		super();
		this.id = id;
		this.member_id = member_id;
		this.group_id = group_id;
		this.stay_name = stay_name;
		this.experience_name = experience_name;
		this.code = code;
		this.reservation_date = reservation_date;
		this.reservation_person = reservation_person;
		this.price = price;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getMember_id() {
		return member_id;
	}

	public void setMember_id(String member_id) {
		this.member_id = member_id;
	}

	public int getGroup_id() {
		return group_id;
	}

	public void setGroup_id(int group_id) {
		this.group_id = group_id;
	}

	public String getStay_name() {
		return stay_name;
	}

	public void setStay_name(String stay_name) {
		this.stay_name = stay_name;
	}

	public String getExperience_name() {
		return experience_name;
	}

	public void setExperience_name(String experience_name) {
		this.experience_name = experience_name;
	}

	public int getCode() {
		return code;
	}

	public void setCode(int code) {
		this.code = code;
	}

	public String getReservation_date() {
		return reservation_date;
	}

	public void setReservation_date(String reservation_date) {
		this.reservation_date = reservation_date;
	}

	public int getReservation_person() {
		return reservation_person;
	}

	public void setReservation_person(int reservation_person) {
		this.reservation_person = reservation_person;
	}

	public int getPrice() {
		return price;
	}

	public void setPrice(int price) {
		this.price = price;
	}

	@Override
	public String toString() {
		return "Reservation [id=" + id + ", member_id=" + member_id + ", group_id=" + group_id + ", stay_name="
				+ stay_name + ", experience_name=" + experience_name + ", code=" + code + ", reservation_date="
				+ reservation_date + ", reservation_person=" + reservation_person + ", price=" + price + "]";
	}
	
	
}
